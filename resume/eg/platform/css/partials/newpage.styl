.page
	width 100%
	min-height 370px
	height auto
	background-color page-bg
	border 1px solid border-color
	border-radius 5px
	box-shadow 3px 3px 5px #666
	box-sizing border-box
	-webkit-box-sizing border-box
	-moz-box-sizing border-box
	padding 40px 30px

#newpageBtn
	width 270px
	height 55px		
	line-height 55px
	background-color header-bg
	color #fff
	text-align center
	position absolute
	left 0px
	right 0px
	top 0px
	bottom 0px
	margin auto
	border-radius 3px
	cursor pointer
